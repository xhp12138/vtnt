{
  "name": "vtnt",
  "version": "1.0.0",
  "description": "a common components library",
  "scripts": {
    "dev": "npm run create && webpack-dev-server --config build/webpack.dev.conf.js",
    "build": "npm run create && cross-env NODE_ENV=production  webpack --config ./build/webpack.build.conf.js && npm run build:other && npm run build:components",
    "build:components": "cross-env NODE_ENV=production webpack --config ./build/webpack.components.conf.js",
    "build:other": "babel src --out-dir lib --ignore 'src/components','src/styles','src/global.d.ts' --extensions '.ts,.js'",
    "create": "node ./build/create-components",
    "debug:dev": "npm run create && node --inspect-brk node_modules/webpack-dev-server/bin/webpack-dev-server.js --config build/webpack.dev.conf.js",
    "debug:build": "npm run create && node --inspect-brk node_modules/webpack/bin/webpack.js --config ./build/webpack.build.conf.js",
    "test": "node ./build/test"
  },
  "keywords": [
    "vue",
    "components",
    "typescript"
  ],
  "dependencies": {
    "async-validator": "^3.2.3",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vue-tsx-support": "^2.3.2",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-syntax-jsx": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.5.1",
    "@types/node": "^13.9.1",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "babel-loader": "^8.0.6",
    "chalk": "^4.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.3.2",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.0.1",
    "eslint-config-elemefe": "^0.3.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.2.0",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^3.2.0",
    "json-templater": "^1.2.0",
    "kebab-case": "^1.0.0",
    "marked": "^0.8.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "typescript": "^3.7.3",
    "url-loader": "^3.0.0",
    "vue-loader": "^15.7.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "813531550@qq.com",
  "license": "ISC",
  "main": "lib/index.js",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
